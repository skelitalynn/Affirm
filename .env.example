# Affirm项目 - 环境变量配置
# 复制此文件为 .env 并填写实际值

# 数据库配置
DB_URL=postgresql://username:password@localhost:5432/affirm_db

# Telegram配置
TELEGRAM_BOT_TOKEN=你的telegram_bot_token

# Notion配置
NOTION_TOKEN=你的notion_integration_token
NOTION_PARENT_PAGE_ID=你的notion_parent_page_id

# Gemini AI配置
GEMINI_API_KEY=你的gemini_api_key
MODEL_NAME=gemini-3-flash

# 项目配置
TIMEZONE=Asia/Shanghai
LOG_LEVEL=info
NODE_ENV=development

# OpenClaw配置（如果使用）
OPENCLAW_GATEWAY_URL=http://localhost:18789
OPENCLAW_GATEWAY_TOKEN=你的gateway_token

# 安全配置（生产环境需要）
JWT_SECRET=你的jwt_secret_key
ENCRYPTION_KEY=你的加密密钥

# GitHub配置（用于自动提交）
GITHUB_TOKEN=你的github_personal_access_token
GITHUB_USERNAME=你的github用户名
GITHUB_REPO=你的仓库名